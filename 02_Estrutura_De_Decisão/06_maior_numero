num1 = int(input("Digite a primara nota: "))
num2 = int(input("Digite a segunda nota: "))
num3 = int(input("Digite a terceira nota: "))
if(num1 > num2 and num1 > num3):
    print("O número {} é o maior".format(num1))
elif(num2 > num1 and num2 > num3):
    print("O número {} é o maior".format(num2))
else:
    print("O numero {} é o maior".format(num3))
